  "paths": {
    "/api/Auth/Login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "operationId": "Auth_Login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Stt.Etg.OrdersConfirmationAPI.Models.LoginModel"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Stt.Etg.OrdersConfirmationAPI.Models.LoginResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input or server error"
          }
        }
      }
    },
    "/api/trades": {
      "get": {
        "tags": [
          "Trade"
        ],
        "operationId": "Trade_GetUnconfirmedTrades",
        "parameters": [
          {
            "name": "tradeId",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "counterparty",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Stt.Etg.OrdersConfirmationAPI.Models.Trade"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input or server error"
          }
        }
      }
    },
    "/api/trades/confirmation": {
      "post": {
        "tags": [
          "Trade"
        ],
        "operationId": "Trade_ConfirmTrade",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Stt.Etg.OrdersConfirmationAPI.Models.TradeConfirmationList"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Stt.Etg.OrdersConfirmationAPI.Models.Trade"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input or server error"
          }
        }
      }
    }
  },
